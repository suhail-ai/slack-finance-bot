{
  "version": 2,
  "builds": [
    {
      "src": "api/slack.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/slack",
      "dest": "/api/slack.js",
      "methods": ["POST", "GET"],
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ]
}
